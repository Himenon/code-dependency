{
  "name": "@code-dependency/view",
  "version": "0.0.1-alpha.2",
  "description": "A user interface for displaying code dependencies on a browser.",
  "keywords": [],
  "homepage": "https://github.com/https://github.com/Himenon/code-dependency.git#readme",
  "bugs": {
    "url": "https://github.com/https://github.com/Himenon/code-dependency.git/issues"
  },
  "repository": "https://github.com/Himenon/code-dependency.git",
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": ""
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "build",
    "package.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "rimraf build && yarn run build:scripts && node scripts/build.js",
    "build:scripts": "tsc -p tsconfig.scripts.json",
    "build:style": "webpack --config ./scripts/style.js",
    "ci:notify:release": "ts-node ./scripts/notify.ts",
    "clean": "rimraf ./lib ./build",
    "cli": "node ./lib/cli.js",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "cruiser": "NODE_ENV=\"production\" npx dependency-cruiser --validate .dependency-cruiser.json --webpack-config ./scripts/webpack/webpack.config.js --ts-config=./tsconfig.json src",
    "format": "prettier --config ../../.prettierrc --write src/*.{ts,tsx}",
    "lint": "eslint --cache --cache-location ../../buildcache/view/ -c ../../.eslintrc.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint --fix",
    "develop": "yarn run build:scripts && node scripts/develop.js",
    "server:prod": "serve -s build",
    "start": "yarn run develop",
    "test": "yarn run cruiser && yarn run test:jest",
    "test:ci": "yarn run build:scripts && yarn run test && codecov",
    "test:jest": "jest --cache -c ./jest.config.json",
    "test:watch": "yarn run test:jest --watch",
    "clean:lib": "rimraf lib"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@code-dependency/converter": "^0.0.1-alpha.2",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "d3": "^5.9.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@code-dependency/interfaces": "^0.0.1-alpha.2",
    "@types/classnames": "^2.2.7",
    "@types/d3": "^5.7.2",
    "@types/glob": "^7.1.1",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jsdom": "^12.2.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/react": "^16.8.13",
    "@types/react-dev-utils": "^7.0.1",
    "@types/react-dom": "^16.8.4",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "^4.4.27",
    "@types/webpack-dev-server": "^3.1.5",
    "@types/webpack-manifest-plugin": "^2.0.0",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "^6.1.1",
    "cache-loader": "^2.0.1",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "css-loader": "^2.1.1",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "fibers": "^3.1.1",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin-alt": "^0.4.14",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "jest-pnp-resolver": "^1.2.1",
    "jsdom": "^14.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pnp-webpack-plugin": "^1.4.1",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "postcss-safe-parser": "^4.0.1",
    "react-dev-utils": "^8.0.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "typescript": "^3.4.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "~3.2.1",
    "webpack-manifest-plugin": "^2.0.4",
    "workbox-webpack-plugin": "^4.2.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
