{
  "name": "@code-dependency/cli",
  "version": "0.0.1",
  "description": "import view",
  "keywords": [],
  "homepage": "https://github.com/Himenon/code-dependency#readme",
  "bugs": {
    "url": "https://github.com/Himenon/code-dependency/issues"
  },
  "repository": "https://github.com/Himenon/code-dependency/packages/cli",
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://github.com/Himenon"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "bin": {
    "code-dependency": "./lib/index.js"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "clean": "rimraf ./lib && tsc -b . --clean",
    "cli": "node ./lib/index.js -f ../code-dependency/src/index.ts",
    "cli:server": "node ./lib/index.js -s -p ../code-dependency/src/index.ts -c",
    "cruiser": "dependency-cruiser --validate .dependency-cruiser.json src",
    "format": "prettier --config ../../.prettierrc --write src/*.{ts,tsx}",
    "lint": "eslint --cache --cache-location buildcache -c ../../.eslintrc.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint --fix",
    "server": "node ./lib/server.js",
    "start": "yarn run develop",
    "test": "yarn run cruiser && yarn run test:jest",
    "test:ci": "yarn run test && codecov",
    "test:jest": "jest --cache -c ./jest.config.json",
    "test:watch": "yarn run test:jest --watch"
  },
  "dependencies": {
    "@code-dependency/code-dependency": "0.0.1",
    "@code-dependency/interfaces": "0.0.1",
    "@code-dependency/resolver": "0.0.1",
    "@code-dependency/converter": "0.0.1",
    "@code-dependency/view": "0.0.1",
    "commander": "^2.20.0",
    "express": "^4.16.4"
  },
  "devDependencies": {
    "@types/express": "^4.16.1"
  }
}
