{
  "name": "@code-dependency/resolver",
  "version": "0.0.1-alpha.2",
  "description": "It is a library to classify extracted modules and attach information to the result.",
  "keywords": [],
  "homepage": "https://github.com/Himenon/code-dependency#readme",
  "bugs": {
    "url": "https://github.com/Himenon/code-dependency/issues"
  },
  "repository": "https://github.com/Himenon/code-dependency/packages/resolver",
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://github.com/Himenon"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf ./lib ./build",
    "cli": "node ./lib/cli/index.js -f ./src/index.ts",
    "cruiser": "dependency-cruiser --validate .dependency-cruiser.json src",
    "format": "prettier --config ../../.prettierrc --write src/*.{ts,tsx}",
    "lint": "eslint --cache --cache-location ../../buildcache/resolver/ -c ../../.eslintrc.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint --fix",
    "server": "node ./lib/server.js",
    "start": "yarn run develop",
    "test": "yarn run cruiser && yarn run test:jest",
    "test:ci": "yarn run test && codecov",
    "test:jest": "jest --cache -c ./jest.config.json",
    "test:watch": "yarn run test:jest --watch",
    "clean:lib": "rimraf lib"
  },
  "dependencies": {
    "@code-dependency/interfaces": "^0.0.1-alpha.2",
    "enhanced-resolve": "^4.1.0",
    "lodash": "^4.17.11",
    "normalize-package-data": "^2.5.0",
    "pnp-webpack-plugin": "^1.4.1",
    "read-package-json": "^2.0.13",
    "resolve": "^1.10.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0"
  },
  "devDependencies": {
    "@code-dependency/test-project": "^0.0.1-alpha.2",
    "@types/normalize-package-data": "^2.4.0"
  },
  "peerDependencies": {
    "graceful-fs": "^4.1.2"
  }
}
