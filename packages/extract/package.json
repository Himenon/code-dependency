{
  "name": "@code-dependency/extract",
  "version": "0.0.1-alpha.0",
  "description": "Library for extracting dependencies from specific files.",
  "keywords": [],
  "homepage": "https://github.com/Himenon/code-dependency#readme",
  "bugs": {
    "url": "https://github.com/Himenon/code-dependency/issues"
  },
  "repository": "https://github.com/Himenon/code-dependency/packages/extract",
  "license": "MIT",
  "author": {
    "name": "Himenon",
    "email": "k.himeno314@gmail.com",
    "url": "https://github.com/Himenon"
  },
  "files": [
    "lib",
    "package.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "clean": "rimraf ./lib ./build",
    "cli": "node ./lib/cli/index.js -f ./src/index.ts",
    "cruiser": "dependency-cruiser --validate .dependency-cruiser.json src",
    "format": "prettier --config ../../.prettierrc --write src/*.{ts,tsx}",
    "lint": "eslint --cache --cache-location ../../buildcache/extract/ -c ../../.eslintrc.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint --fix",
    "server": "node ./lib/server.js",
    "start": "yarn run develop",
    "test": "yarn run cruiser && yarn run test:jest",
    "test:ci": "yarn run test && codecov",
    "test:jest": "jest --cache -c ./jest.config.json",
    "test:watch": "yarn run test:jest --watch",
    "clean:lib": "rimraf lib"
  },
  "dependencies": {
    "resolve": "^1.10.0",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@code-dependency/interfaces": "^0.0.1-alpha.0",
    "@code-dependency/test-project": "^0.0.1-alpha.0"
  }
}
