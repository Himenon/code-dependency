{
  "name": "@code-dependency/view",
  "version": "0.1.0",
  "homepage": "https://pages.github.com/Himenon/code-dependency",
  "bugs": "https://github.com/Himenon/code-dependency/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:Himenon/code-dependency.git"
  },
  "license": "MIT",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=4096 yarn ts ./scripts/build.ts",
    "commit": "npx git-cz",
    "depcruise": "depcruise --validate .dependency-cruiser.js src",
    "depcruise:html": "depcruise --validate --output-type err-html -f dependency-report.html .dependency-cruiser.js src",
    "deploy": "yarn build && yarn publish:site",
    "format": "yarn lint --fix && prettier --write \"**/*.{ts,tsx,js,jsx,json,yml,yaml,html,md}\"",
    "lint": "eslint -c ./.eslintrc.js 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --config .prettierrc --write src/**/*",
    "publish:site": "yarn ./scripts/publish.ts",
    "serve": "serve dist",
    "start": "cross-env NODE_ENV=development yarn ts ./scripts/develop.ts",
    "test": "yarn test:jest && yarn depcruise",
    "test:jest": "jest -c ./jest.config.json",
    "test:jest:watch": "yarn test:jest --watch",
    "ts": "ts-node -P ./scripts/tsconfig.cli.json"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "viz.js": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/gh-pages": "^2.0.1",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/terser-webpack-plugin": "^2.2.0",
    "@types/webpack": "^4.39.8",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "@types/webpack-dev-server": "^3.4.0",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.0.3",
    "conventional-changelog-angular-all": "^1.7.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "3",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "dependency-cruiser": "^6.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fibers": "^4.0.2",
    "fork-ts-checker-notifier-webpack-plugin": "^1.0.3",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "gh-pages": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "regenerator-runtime": "^0.13.3",
    "resolve-pkg": "^2.0.0",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "serve": "^11.2.0",
    "sort-package-json": "^1.32.2",
    "style-loader": "^1.1.2",
    "styled-components": "^4.4.1",
    "terser-webpack-plugin": "^2.3.1",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-notifier": "^1.8.0"
  }
}
