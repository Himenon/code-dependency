{
  "compilerOptions": {
    "outDir": "lib",
    "module": "commonjs",
    "target": "esnext",
    "lib": ["dom", "esnext"],
    "experimentalDecorators": true,
    "removeComments": true,
    "sourceMap": true,
    "allowJs": false,
    "moduleResolution": "node",
    "rootDir": "./src",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strict": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "baseUrl": "./",
    "noLib": false,
    "declaration": true,
    "jsx": "react",
    "tsBuildInfoFile": "../../buildcache/test-project/tsconfig.json.tsbuildinfo"
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["./node_modules"],
  "ordered-imports": [
    true,
    {"import-sources-order": "any", "named-imports-order": "case-insensitive"}
  ],
  "extends": "../tsconfig.shared"
}
