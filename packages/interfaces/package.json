  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf ./lib ./build",
    "cli": "node ./lib/cli/index.js -f ./src/index.ts",
    "lint": "eslint --cache --cache-location ../../buildcache/interfaces/ -c ../../.eslintrc.json 'src/**/*.{ts,tsx}'",
    "lint:fix": "yarn run lint --fix",
    "server": "node ./lib/server.js",
    "start": "yarn run develop",
    "test": "yarn run test:jest",
    "test:ci": "yarn run test && codecov",
    "test:jest": "jest --cache -c ./jest.config.json",
    "test:watch": "yarn run test:jest --watch",
    "clean:lib": "rimraf lib"
  }
